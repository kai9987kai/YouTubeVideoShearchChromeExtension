{
	"name": "Star Wars Nuggets Searching Tool",
	"version": "0.1",
	"manifest_version": 2,
	"description": "A chrome extension for searching my YouTube channels videos",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Star Wars Nuggets Searching Tool",
		"default_popup": "extension.html"
	},
	"content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
	"icons": {
		"128": "icon.png"
	}
}
